{
  "expo": {
    "name": "SmartMind",
    "slug": "SmartMind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/SmartMind.png",
    "scheme": "smartmind",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fahdlabba.SmartMind",
      "infoPlist": {
        "NSCalendarsUsageDescription": "This app needs access to your calendar to manage and view events",
        "NSRemindersUsageDescription": "This app needs access to your reminders to manage tasks"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.fahdlabba.SmartMind",
      "permissions": [
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-calendar",
        {
          "calendarPermission": "The app needs to access your calendar to manage events."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b47c5e91-f894-43c6-b931-7f9f9df4d6f0"
      }
    }
  }
}
